    //买飞机票
    public static void tourist(){
        Scanner sc = new Scanner(System.in);
        System.out.println("请输入机票原价：");
        double ticket = sc.nextInt();
        System.out.println("请输入月份：");
        int month = sc.nextInt();
        System.out.println("请输入舱位（0——头等舱，1——经济舱）：");
